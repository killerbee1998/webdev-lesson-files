var user = [
    {
	name: 'riasat',
	pass: '123456',
    },
    {
	name: 'mahee',
	pass: 'haccerman',
    },
];

var timeline = [
    {
	msg: 'Your birthday is tommrow Riasat. Happy Birthday',
    },
    {
	msg: 'It\'s Riasat\'s birthday Mahee. Wish him a happy birthday',
    }
];



function check(u,p){
    if(u == user[0].name && p == user[0].pass){
	alert(timeline[0].msg);
    }else if(u == user[1].name && p == user[1].pass){
	    alert(timeline[1].msg);
    }else{
	alert("Wrong username or password. Try again");
    }
};

var username = prompt('What\'s your username');
var password = prompt('What\'s your password');

check(username,password);

